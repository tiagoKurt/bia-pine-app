{
    "name": "bia-pine-app/php",
    "description": "Aplicativo PHP BIA/PINE convertido do Python",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.10",
        "phpoffice/phpword": "^1.0",
        "vlucas/phpdotenv": "^5.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "smalot/pdfparser": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public",
        "test": "php -l src/*.php",
        "clean": "rm -rf vendor composer.lock && composer install"
    }
}
