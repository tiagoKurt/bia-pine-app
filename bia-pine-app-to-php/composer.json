{
    "name": "bia-pine-app/php",
    "description": "Aplicativo PHP BIA/PINE convertido do Python",
    "type": "project",
    "require": {
        "php": ">=8.3",
        "guzzlehttp/guzzle": "^7.0",
        "phpoffice/phpword": "^1.0",
                "google/apiclient": "^2.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
